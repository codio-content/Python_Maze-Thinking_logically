<script>

function loadUserScript(fileName) {
  $.get(fileName)
  .success(function(data) {
        
    if(data == 404) {
      return swal({ 
        title: "404",
        text: fileName + " not found",
        html: false,
        confirmButtonText: "OK"
      }); 
    }

    try {        
      var module = Sk.importMainWithBody("", false, data);
      
      if (typeof userScriptLoaded !== 'undefined') {
        userScriptLoaded()
      }
    }
    catch(e) {
      swal({ 
        title: "Python Error",
        text: e.toString(),
        html: false,
        confirmButtonText: "OK"
      }); 
    }
  }).fail(function(jqxhr, settings, e) {
    swal({ 
      title: "Error",
      text: e.message,
      html: false,
      confirmButtonText: "OK"
    });
  });
}
  
</script>